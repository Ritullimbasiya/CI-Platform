<!-- Add Mission -->
<div class="offcanvas offcanvas-start" data-bs-backdrop="static" tabindex="-1" id="addmission"
     aria-labelledby="addmissionLabel">
    <div class="offcanvas-header">
        <h5 class="offcanvas-title" id="addmission">Add New Mission</h5>
        <button type="button" class="btn-close" data-bs-dismiss="offcanvas" aria-label="Close"></button>
    </div>
    <div class="offcanvas-body">
        <div>
            <div class="row" style="margin:18px 0px;font-size: 10px;">
                <div class="col-6" style="padding:0px 15px 0px 0px;">
                    <label for="Country">City*</label><br>
                    <select name="Country" id="Country" class="sherescontainer1">
                        <option value="">Select your City</option>
                        <option value="India">Amreli</option>
                        <option value="Italy">Rajkot</option>
                        <option value="Switzerland">Surat</option>
                        <option value="Canada">Ahmedabad</option>
                    </select>
                </div>
                <div class="col-6" style="padding:0px 15px 0px 0px;">
                    <label for="Country">Country*</label><br>
                    <select name="Country" id="Country" class="sherescontainer1">
                        <option value="">Select your Country</option>
                        <option value="India">India</option>
                        <option value="Italy">Italy</option>
                        <option value="Switzerland">Switzerland</option>
                        <option value="Canada">Canada</option>
                        <option value="Ireland">Ireland</option>
                    </select>
                </div>
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="Mission Title" style="padding:0px;">Mission Title</label><br>
                <input type="text" id="Mission Title" name="Mission Title" placeholder="Enter Mission Title"
                       class="sherescontainer1">
            </div>
            <div class="row" style="margin:18px 0px; font-size: 10px;">
                <label for="Mission Description" style="padding:0px;">Mission Description</label><br>
                <textarea id="Mission Description" name="Mission Description" placeholder="Enter your message..."
                          style="width:100%;height:100px;border: 1px solid #d9d9d9;box-shadow: none;resize: none;"></textarea>
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="mon" style="padding:0px;">Mission Organisation Name</label><br>
                <input type="text" id="mon" name="mon" placeholder="Enter mission organisation name"
                       class="sherescontainer1">
            </div>
            <div class="row" style="margin:18px 0px; font-size: 10px;">
                <label for="mod" style="padding:0px;">Mission Organisation Detail</label><br>
                <textarea id="mod" name="mod" placeholder="Enter your message"
                          style="width:100%;height:100px;border: 1px solid #d9d9d9;box-shadow: none;resize: none;"></textarea>
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="Start Date" style="padding:0px;">Start Date</label><br>
                <input type="Date" id="Start Date" name="Start Date" class="sherescontainer1">
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="End Date" style="padding:0px;">End Date</label><br>
                <input type="date" id="End Date" name="End Date" class="sherescontainer1">
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="Total seats" style="padding:0px;">Total seats</label><br>
                <input type="text" id="Total seats" name="Total seats" placeholder="Enter total seats"
                       class="sherescontainer1">
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="mrd" style="padding:0px;">Mission registration deadline</label><br>
                <input type="text" id="mrd" name="mrd" placeholder="Enter mission registration deadline"
                       class="sherescontainer1">
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="mSkills" style="padding:0px;">Mission Skills</label><br>
                <select name="mSkills" id="mSkills" class="sherescontainer1">
                    <option value="">Select mission Skills</option>
                    <option value="Anthropology">Anthropology</option>
                    <option value="Archeology">Archeology</option>
                    <option value="se">Science Environmental </option>
                    <option value="Science">Science</option>
                    <option value="History">History</option>
                    <option value="Mathematics">Mathematics</option>
                </select>
            </div>
            <div class="row" style="padding:0px 15px 0px 0px;margin:18px 0px;font-size: 10px;">
                <label for="mTheme" style="padding:0px;">Mission Theme</label><br>
                <select name="mTheme" id="mTheme" class="sherescontainer1">
                    <option value="">Select mission theme</option>
                    <option value="Newest">Newest</option>
                    <option value="Oldest">Oldest</option>
                    <option value="Lowest">Lowest available seats</option>
                    <option value="Highest">Highest available seats</option>
                    <option value="fevorite">Short by my fevorite</option>
